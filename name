import { ApiEmailContactDto } from "./api-email-contact-dto";
import { ApiFeatureDto } from "./api-feature-dto";

export class ApiGeneralSettingDto {
    private _product:string;
    private _globalContacts:ApiEmailContactDto[];
    private _features:ApiFeatureDto[];


    /**
     * Getter product
     * @return {string}
     */
	public get product(): string {
		return this._product;
	}

    /**
     * Getter globalContacts
     * @return {ApiEmailContactDto[]}
     */
	public get globalContacts(): ApiEmailContactDto[] {
		return this._globalContacts;
	}

    /**
     * Getter features
     * @return {ApiFeatureDto[]}
     */
	public get features(): ApiFeatureDto[] {
		return this._features;
	}

    /**
     * Setter product
     * @param {string} value
     */
	public set product(value: string) {
		this._product = value;
	}

    /**
     * Setter globalContacts
     * @param {ApiEmailContactDto[]} value
     */
	public set globalContacts(value: ApiEmailContactDto[]) {
		this._globalContacts = value;
	}

    /**
     * Setter features
     * @param {ApiFeatureDto[]} value
     */
	public set features(value: ApiFeatureDto[]) {
		this._features = value;
	}

    
}
