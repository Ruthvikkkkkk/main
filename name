import { ApiUserStoryComment } from './api-user-story-comment';

describe('ApiUserStoryComment', () => {
  let apiUserStoryComment: ApiUserStoryComment;

  beforeEach(() => {
    apiUserStoryComment = new ApiUserStoryComment();
  });

  it('should create an instance', () => {
    expect(apiUserStoryComment).toBeTruthy();
  });

  it('should have default values set', () => {
    expect(apiUserStoryComment.comment).toBeUndefined();
    expect(apiUserStoryComment.date).toBeUndefined();
    expect(apiUserStoryComment.commentedBy).toBeUndefined();
  });

  it('should set and get comment correctly', () => {
    const mockComment = 'Sample Comment';

    apiUserStoryComment.comment = mockComment;

    expect(apiUserStoryComment.comment).toEqual(mockComment);
  });

  it('should set and get date correctly', () => {
    const mockDate = 'Sample Date';

    apiUserStoryComment.date = mockDate;

    expect(apiUserStoryComment.date).toEqual(mockDate);
  });

  it('should set and get commentedBy correctly', () => {
    const mockCommentedBy = 'Sample Commented By';

    apiUserStoryComment.commentedBy = mockCommentedBy;

    expect(apiUserStoryComment.commentedBy).toEqual(mockCommentedBy);
  });

  // Add more test cases as needed
});
