import { ApiUserStoryComment } from "./api-user-story-comment-dto";
import { ApiUserStoryDocument } from "./api-user-story-document-dto";
import { ApiUserStoryNotes } from "./api-user-story-notes-dto";

export class ApiUserStoryDetails {
    
    private _storyName: string;
    private _storyuuid: string;
    private _state: string;
    private _isEditAllowed: boolean;
    private _isApproveAndSubmitVisible: boolean;
    private _supportText: string;
    private _assignedInfo: string;
    private _description: string;
    
    private _notes: ApiUserStoryNotes;
    private _documentsList: ApiUserStoryDocument[];
    private _commentsList: ApiUserStoryComment[];

    /**
     * Getter storyName
     * @return {string}
     */
	public get storyName(): string {
		return this._storyName;
	}

    /**
     * Getter storyuuid
     * @return {string}
     */
	public get storyuuid(): string {
		return this._storyuuid;
	}

    /**
     * Getter state
     * @return {string}
     */
	public get state(): string {
		return this._state;
	}

    /**
     * Getter isEditAllowed
     * @return {boolean}
     */
	public get isEditAllowed(): boolean {
		return this._isEditAllowed;
	}

    /**
     * Getter isApproveAndSubmitVisible
     * @return {boolean}
     */
	public get isApproveAndSubmitVisible(): boolean {
		return this._isApproveAndSubmitVisible;
	}

    /**
     * Getter supportText
     * @return {string}
     */
	public get supportText(): string {
		return this._supportText;
	}

    /**
     * Getter assignedInfo
     * @return {string}
     */
	public get assignedInfo(): string {
		return this._assignedInfo;
	}

    /**
     * Getter description
     * @return {string}
     */
	public get description(): string {
		return this._description;
	}

    /**
     * Getter notes
     * @return {ApiUserStoryNotes}
     */
	public get notes(): ApiUserStoryNotes {
		return this._notes;
	}

    /**
     * Getter documentsList
     * @return {ApiUserStoryDocument[]}
     */
	public get documentsList(): ApiUserStoryDocument[] {
		return this._documentsList;
	}

    /**
     * Getter commentsList
     * @return {ApiUserStoryComment[]}
     */
	public get commentsList(): ApiUserStoryComment[] {
		return this._commentsList;
	}

    /**
     * Setter storyName
     * @param {string} value
     */
	public set storyName(value: string) {
		this._storyName = value;
	}

    /**
     * Setter storyuuid
     * @param {string} value
     */
	public set storyuuid(value: string) {
		this._storyuuid = value;
	}

    /**
     * Setter state
     * @param {string} value
     */
	public set state(value: string) {
		this._state = value;
	}

    /**
     * Setter isEditAllowed
     * @param {boolean} value
     */
	public set isEditAllowed(value: boolean) {
		this._isEditAllowed = value;
	}

    /**
     * Setter isApproveAndSubmitVisible
     * @param {boolean} value
     */
	public set isApproveAndSubmitVisible(value: boolean) {
		this._isApproveAndSubmitVisible = value;
	}

    /**
     * Setter supportText
     * @param {string} value
     */
	public set supportText(value: string) {
		this._supportText = value;
	}

    /**
     * Setter assignedInfo
     * @param {string} value
     */
	public set assignedInfo(value: string) {
		this._assignedInfo = value;
	}

    /**
     * Setter description
     * @param {string} value
     */
	public set description(value: string) {
		this._description = value;
	}

    /**
     * Setter notes
     * @param {ApiUserStoryNotes} value
     */
	public set notes(value: ApiUserStoryNotes) {
		this._notes = value;
	}

    /**
     * Setter documentsList
     * @param {ApiUserStoryDocument[]} value
     */
	public set documentsList(value: ApiUserStoryDocument[]) {
		this._documentsList = value;
	}

    /**
     * Setter commentsList
     * @param {ApiUserStoryComment[]} value
     */
	public set commentsList(value: ApiUserStoryComment[]) {
		this._commentsList = value;
	}

}    
