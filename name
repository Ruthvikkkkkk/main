export class ApiUserStoryComment{
   private _comment: string;
   private _date: string; 
   private _commentedBy: string;

    /**
     * Getter comment
     * @return {string}
     */
	public get comment(): string {
		return this._comment;
	}

    /**
     * Getter date
     * @return {string}
     */
	public get date(): string {
		return this._date;
	}

    /**
     * Getter commentedBy
     * @return {string}
     */
	public get commentedBy(): string {
		return this._commentedBy;
	}

    /**
     * Setter comment
     * @param {string} value
     */
	public set comment(value: string) {
		this._comment = value;
	}

    /**
     * Setter date
     * @param {string} value
     */
	public set date(value: string) {
		this._date = value;
	}

    /**
     * Setter commentedBy
     * @param {string} value
     */
	public set commentedBy(value: string) {
		this._commentedBy = value;
	}

}
